{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "check-dependencies": "check-dependencies --install",
    "clean": "rimraf dist & rimraf build",
    "validate-webpack": "webpack-validator ./webpack/dev.config.js",
    "validate-webpack-prod": "webpack-validator ./webpack/prod.config.js",
    "test": "karma start",
    "prebuild": "npm run check-dependencies && npm run clean",
    "prebuild-prod": "npm run clean && npm run check-dependencies",
    "build": "webpack --config ./webpack/dev.config.js ",
    "build-prod": "webpack --config ./webpack/prod.config.js",
    "watch": "webpack --config ./webpack/dev.config.js --watch",
    "watch-prod": "webpack --config ./webpack/prod.config.js --watch",
    "profile": "npm run build --profile --json > ./stats/dev.json",
    "profile-prod": "npm run build-prod --profile --json > ./stats/prod.json",
    "prestart": "npm run build",
    "prestart-prod": "npm run build-prod",
    "start": "webpack-dev-server --inline --open --config ./webpack/dev.config.js --host 0.0.0.0",
    "start-prod": "webpack-dev-server --open --config ./webpack/prod.config.js --cache=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alelau/webpack-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alelau/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/alelau/webpack-boilerplate#readme",
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.0-rc.5",
    "angular-messages": "1.5.8",
    "angular-ui-router": "0.2.18",
    "css-element-queries": "0.3.2",
    "jquery": "3.1.0",
    "lodash": "4.13.1",
    "ng-device-detector": "3.0.1",
    "oclazyload": "1.0.9",
    "restangular": "1.5.2",
    "event-source-polyfill":"git@bitbucket.org:factor8/eventsource.git#f8-0.0.8",
    "ng-file-upload": "12.0.4",
    "angular-elastic": "2.5.1",
    "bootstrap-css-only": "3.3.6"
  },
  "devDependencies": {
    "autoprefixer":"6.4.0",
    "babel": "6.5.2",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "0.111111.11",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-stage-2": "6.5.0",
    "chai": "3.5.0",
    "check-dependencies": "0.12.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "url-loader": "0.5.7",
    "eslint": "2.9.0",
    "eslint-config-kentcdodds": "6.2.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import":"1.14.0",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "karma": "1.1.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-mocha": "1.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "2.5.3",
    "postcss-loader": "0.9.1",
    "rimraf": "2.5.2",
    "source-map-loader": "0.1.5",
    "string-replace-loader": "1.0.3",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.20",
    "webpack-config": "4.0.0",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.7"
  }
}
